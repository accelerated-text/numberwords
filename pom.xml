      <url>https://clojars.org/repo</url>
    </repository>
  </distributionManagement>
    
  <scm>
        
    <url>https://github.com/tokenmill/numberwords</url>
        
    <connection>scm:git:git://github.com/tokenmill/numberwords.git</connection>
        
